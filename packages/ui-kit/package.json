{
    "name": "ui-kit",
    "version": "0.0.0",
    "source": "lib/index.js",
    "private": true,
    "scripts": {
        "start": "storybook dev -p 6006 -s src/common/assets",
        "build": "rimraf lib && tsc && tsc-alias && postcss src/styles --dir lib/styles --config build-config && copyfiles -u 1 src/**/*.svg lib/ && copyfiles -u 1 src/**/*.png lib/ && copyfiles -u 1 src/libs/**/* lib/",
        "build-storybook": "rimraf storybook-static && storybook build",
        "generate-icons": "svgr --config-file ./svgr.config.js --svgo-config ./svgo.config.js -- ./src/color-icon/svg",
        "pregenerate-icons": "rimraf src/color-icon/components",
        "start-http-server": "rimraf storybook-static && storybook build --quiet && pnpm dlx http-server storybook-static --port 6006 --silent",
        "wait-on-test-storybook": "pnpm dlx wait-on tcp:6006 && test-storybook",
        "test:stories": "concurrently \"pnpm run test:a11y\" \"pnpm run test:components\"",
        "test:a11y": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,green\" \"pnpm run start-http-server\" \"pnpm run wait-on-test-storybook\"",
        "test:components-coverage": "jest --testPathPattern=src --coverage",
        "test:watch": "pnpm run test:components -- --watch",
        "circular-check": "dpdm src/* -T true --tree false --warning false --tsconfig tsconfig.json --exit-code circular:1",
        "lint": "pnpm run eslint && pnpm run prettier:check",
        "lint:fix": "pnpm run eslint:fix && pnpm run prettier:fix",
        "eslint": "eslint \"src/*.{ts,tsx}\"",
        "eslint:fix": "pnpm run eslint --fix",
        "prettier:check": "prettier --check .",
        "prettier:fix": "prettier --write ."
    },
    "resolutions": {
        "acorn": "8.8.2"
    },
    "description": "> TODO: description",
    "author": "Николай Колышев <sword.of.destiny2001@mail.ru>",
    "homepage": "https://github.com/nikolay-kolyshev/table-generator-frontend#readme",
    "license": "ISC",
    "main": "lib/index.js",
    "directories": {
        "lib": "lib",
        "test": "tests"
    },
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nikolay-kolyshev/table-generator-frontend.git"
    },
    "bugs": {
        "url": "https://github.com/nikolay-kolyshev/table-generator-frontend/issues"
    },
    "dependencies": {
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-table": "7.8.0",
        "styled-components": "5.3.9",
        "vite": "4.2.1"
    },
    "devDependencies": {
        "@react-theming/storybook-addon": "1.1.10",
